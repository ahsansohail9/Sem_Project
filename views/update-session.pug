                        <select class="form-control" id="add-session-category" name="category" required>
                            <option value="">Select Category</option>
                            if (tempsession.category == "annual")
                                <option selected value="annual">Annual</option>
                                <option value="special">Special</option>
                                <option value="local">Local</option>
                                <option value="on-demand">On Demand</option>
                            else if (tempsession.category == "special")
                                <option value="annual">Annual</option>
                                <option selected value="special">Special</option>
                                <option value="local">Local</option>
                                <option value="on-demand">On Demand</option>
                            else if (tempsession.category == "local")
